{
  "filters": {},
  "plugins": {
        "@textlint/markdown": {
            "extensions": [".mdx"]
        }
    },
  "rules": {
    "@textlint-rule/no-unmatched-pair": true,
    "abbr-within-parentheses": true,
    "alex": true,
    "apostrophe": true,
    "common-misspellings": true,
    "diacritics": true,
    "en-capitalization": true,
    "ginger": false,
    "no-start-duplicated-conjunction": true,
    "period-in-list-item": true,
    "rousseau": true,
    "spelling": false,
    "stop-words": true,
    "terminology": true,
    "unexpanded-acronym": true,
    "write-good": true
  }
}